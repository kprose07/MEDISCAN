<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".Results">


    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/names_for_t"
            android:layout_width="302dp"
            android:layout_height="35dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="26dp"
            android:layout_marginTop="54dp"
            android:fontFamily="@font/poppins"
            android:gravity="top"
            android:lineSpacingExtra="10sp"
            android:text="@string/names_for_t"
            android:textAppearance="@style/names_for_t"
            android:translationY="-5sp" />

        <TextView
            android:id="@+id/brand_names"
            android:layout_width="112dp"
            android:layout_height="21dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="29dp"
            android:layout_marginTop="96dp"
            android:fontFamily="@font/poppins_extrabold"
            android:gravity="top"
            android:lineSpacingExtra="0sp"
            android:text="@string/brand_names"
            android:textAppearance="@style/brand_names" />
        <TextView
            android:id="@+id/tirzepatide"
            android:layout_width="159dp"
            android:layout_height="21dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="141dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="96dp"
            android:text="@string/tirzepatide"
            android:textAppearance="@style/tirzepatide"
            android:lineSpacingExtra="0sp"
            android:gravity="top"
            android:paddingLeft="5dp"
            />

        <TextView
            android:id="@+id/generic_nam"
            android:layout_width="119dp"
            android:layout_height="23dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="29dp"
            android:layout_marginTop="125dp"
            android:fontFamily="@font/poppins_extrabold"
            android:gravity="top"
            android:lineSpacingExtra="0sp"
            android:text="@string/generic_nam"
            android:textAppearance="@style/generic_nam" />
        <TextView
            android:id="@+id/mounjaro"
            android:layout_width="171dp"
            android:layout_height="23dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="148dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="125dp"
            android:text="@string/mounjaros"
            android:textAppearance="@style/mounjaros"
            android:lineSpacingExtra="0sp"
            android:gravity="top"
            />

        <TextView
            android:id="@+id/medication_"
            android:layout_width="122dp"
            android:layout_height="35dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="26dp"
            android:layout_marginTop="156dp"
            android:fontFamily="@font/poppins_mediumbb"
            android:gravity="top"
            android:lineSpacingExtra="10sp"
            android:text="@string/medicationtitle"
            android:textAppearance="@style/medicationtitle"
            android:translationY="-5sp" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerView"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="0dp"
            android:layout_marginTop="299dp"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />
    </RelativeLayout>

</LinearLayout>